{
  "name": "plugin-javascript",
  "version": "2025.3.1",
  "type": "module",
  "exports": "./lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "description": "Javascript plugin for sitespeed.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Webperf-se/plugin-javascript.git"
  },
  "keywords": [
    "sitespeed.io",
    "js",
    "Javascript",
    "webperf",
    "webperf_core",
    "webperf-core"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Webperf-se/plugin-javascript/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=20.x"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "prettier --write --single-quote",
      "git add"
    ]
  },
  "devDependencies": {
    "ava": "6.2.0",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-unicorn": "58.0.0",
    "prettier": "3.5.3"
  },
  "dependencies": {
    "jsdom": "26.0.0",
    "eslint": "9.22.0",
    "globals": "16.0.0",
    "plugin-webperf-core": "2025.3.8",
    "@sitespeed.io/log": "0.2.6",
    "@sitespeed.io/plugin": "1.0.0"
  },
  "scripts": {
    "test": "ava",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "main": "./lib/index.js"
}